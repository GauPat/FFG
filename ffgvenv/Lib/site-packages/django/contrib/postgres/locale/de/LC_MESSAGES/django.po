#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\apps.py:13
msgid "PostgreSQL extensions"
msgstr "PostgreSQL-Erweiterungen"

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\fields\array.py:21
#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\array.py:16
#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\array.py:160
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "Element %(nth)s des Arrays ist ungültig:"

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\fields\array.py:22
msgid "Nested arrays must have the same length."
msgstr "Verschachtelte Arrays müssen die gleiche Länge haben."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\fields\hstore.py:14
msgid "Map of strings to strings/nulls"
msgstr "Zuordnung von Zeichenketten zu Zeichenketten/NULLs"

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\fields\hstore.py:16
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "Der Wert für „%(key)s“ ist keine Zeichenkette oder NULL."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:30
msgid "A JSON object"
msgstr "Ein JSON-Objekt"

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:32
msgid "Value must be valid JSON."
msgstr "Wert muss gültiges JSON sein."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\hstore.py:16
msgid "Could not load JSON data."
msgstr "Konnte JSON-Daten nicht laden."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Eingabe muss ein JSON-Dictionary sein."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\jsonb.py:19
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "„%(value)s“ Wert muss gültiges JSON sein."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:16
msgid "Enter two valid values."
msgstr "Bitte zwei gültige Werte eingeben."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:17
msgid "The start of the range must not exceed the end of the range."
msgstr "Der Anfang des Wertbereichs darf nicht das Ende überschreiten."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:64
msgid "Enter two whole numbers."
msgstr "Bitte zwei ganze Zahlen eingeben."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:70
msgid "Enter two numbers."
msgstr "Bitte zwei Zahlen eingeben."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:76
msgid "Enter two valid date/times."
msgstr "Bitte zwei gültige Datum/Uhrzeit-Werte eingeben."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:82
msgid "Enter two valid dates."
msgstr "Bitte zwei gültige Kalenderdaten eingeben."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\validators.py:14
#, python-format
msgid "List contains %(show_value)d item, it should contain no more than %(limit_value)d."
msgid_plural "List contains %(show_value)d items, it should contain no more than %(limit_value)d."
msgstr[0] "Liste enthält %(show_value)d Element, es sollte aber nicht mehr als %(limit_value)d enthalten."
msgstr[1] "Liste enthält %(show_value)d Elemente, es sollte aber nicht mehr als %(limit_value)d enthalten."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\validators.py:21
#, python-format
msgid "List contains %(show_value)d item, it should contain no fewer than %(limit_value)d."
msgid_plural "List contains %(show_value)d items, it should contain no fewer than %(limit_value)d."
msgstr[0] "Liste enthält %(show_value)d Element, es sollte aber nicht weniger als %(limit_value)d enthalten."
msgstr[1] "Liste enthält %(show_value)d Elemente, es sollte aber nicht weniger als %(limit_value)d enthalten."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Einige Werte fehlen: %(keys)s"

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Einige unbekannte Werte wurden eingegeben: %(keys)s"

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\validators.py:73
#, python-format
msgid "Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "Der Wertebereich muss kleiner oder gleich zu %(limit_value)s sein."

#: .\ffgvenv\Lib\site-packages\django\contrib\postgres\validators.py:79
#, python-format
msgid "Ensure that this range is completely greater than or equal to %(limit_value)s."
msgstr "Der Wertebereich muss größer oder gleich zu %(limit_value)s sein."
